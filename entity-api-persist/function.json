{
  "bindings": [
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post", "put"],
      "authLevel": "anonymous",
      "route": "{owner}/{repo}/shapeshifter/{*proxy}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ]
}